    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "mongose": "^0.0.2-security"
  },
